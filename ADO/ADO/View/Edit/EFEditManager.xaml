<Window x:Class="ADO.View.Edit.EFEditManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADO.View.Edit"
        mc:Ignorable="d"
        Title="EFEditManager" Height="450" Width="800" Loaded="Window_Loaded">
    <StackPanel>
        <Label>Id</Label>
        <TextBox x:Name="Id"></TextBox>
        <Label>Name</Label>
        <TextBox x:Name="Name"></TextBox>
        <Label>Surname</Label>
        <TextBox x:Name="Surname"></TextBox>
        <Label>Secname</Label>
        <TextBox x:Name="Secname"></TextBox>
        <Label>Id_main_dep</Label>
        <TextBox x:Name="Id_main_dep"></TextBox>
        <Label>Id_sec_dep</Label>
        <TextBox x:Name="Id_sec_dep"></TextBox>
        <Label>Id_chief</Label>
        <TextBox x:Name="Id_chief"></TextBox>
        <Label>FiredDt</Label>
        <TextBox x:Name="FiredDt"></TextBox>
        <Button Click="Button_Save" Content="Save"></Button>
        <Button Click="Button_Delete" Content="Delete"></Button>
    </StackPanel>
</Window>
